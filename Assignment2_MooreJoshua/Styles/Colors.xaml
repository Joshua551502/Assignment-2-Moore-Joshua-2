<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!--  Color Palette  #ff333b2c ffffff  #FF383838  -->

    <Color x:Key="LightGrayColor">#FF383838</Color>
    <SolidColorBrush x:Key="LightGrayColorBrush" Color="{StaticResource LightGrayColor}" />

    <Color x:Key="LightWhiteColor">#dad7d0</Color>
    <SolidColorBrush x:Key="LightWhiteColorBrush" Color="{StaticResource LightWhiteColor}" />

    <Color x:Key="DarkGreenColor">#ff333b2c</Color>
    <SolidColorBrush x:Key="DarkGreenColorBrush" Color="{StaticResource DarkGreenColor}" />

    <Color x:Key="GreenColor">#ff333b2c</Color>
    <SolidColorBrush x:Key="GreenColorBrush" Color="{StaticResource GreenColor}" />

    <Color x:Key="CarRentalLightGreen">#FF169758</Color>
    <SolidColorBrush x:Key="CarRentalLightGreenBrush" Color="{StaticResource CarRentalLightGreen}" />

    <Color x:Key="CarRentalDarkGreen">#FF127E49</Color>
    <SolidColorBrush x:Key="CarRentalDarkGreenBrush" Color="{StaticResource CarRentalDarkGreen}" />

    <Color x:Key="ForegroundVeryDark">#000000</Color>
    <SolidColorBrush x:Key="ForegroundVeryDarkBrush" Color="{StaticResource ForegroundVeryDark}" />

    <Color x:Key="ForegroundLight">#ffffff</Color>
    <SolidColorBrush x:Key="ForegroundLightBrush" Color="{StaticResource ForegroundLight}" />
</ResourceDictionary>
